{
    "version": 2,
    "builds": [
        {
            "src": "build/app/app.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["build/**/*"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/app/app.js"
        }
    ]
}
